<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Description>Нужна для удобного возврата результата выполнения методов без использования исключений.</Description>
    <Title>Z1all.ExecutionResult</Title>
    <PackageTags>ExecutionResult OperationResult StatusCode</PackageTags>
    <RepositoryUrl>https://github.com/z1all/ExecutionResult</RepositoryUrl>
    <Authors>z1all</Authors>
    <AssemblyVersion>1.0.0</AssemblyVersion>
    <FileVersion>1.0.0</FileVersion>
    <Version>1.0.0</Version>
  </PropertyGroup>

</Project>
